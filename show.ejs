<div class="container">
    <div class="heading">
        <h1>Welcome to our Blog</h1>
        <a class="btn btn_success"href="/">Home</a>
    </div>


<div class="single_post">
    <h1><%= article.title %></h1>
    <p>
        <%= article.des %>
    </p>
    <b><%= article.info %></b>
    <h5 class="date">
        <i><%= article.createdAt.toLocalDateString() %></i>
    </h5>
</div>

    